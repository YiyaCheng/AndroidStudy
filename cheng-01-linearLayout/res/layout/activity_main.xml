<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" 
        android:background="@drawable/list360">
        <LinearLayout 
            android:layout_width="fill_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            >
            <ImageView 
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/list1"/>
            <ImageView 
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:src="@drawable/listhead"/>
        </LinearLayout>

       
        <LinearLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                >
                <LinearLayout  
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="40dp"
                    android:layout_marginLeft="65dp"
                    android:layout_marginBottom="10dp"
                    android:gravity="center"
                    android:background="#FFFFFF"
                    android:layout_weight="1"
                    android:orientation="vertical">
                    <ImageView 
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/lista"/>
                    <TextView 
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="信号"
                        android:textSize="15sp"
                       />
                 </LinearLayout>
                 <LinearLayout  
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:background="#FFFFFF"
                    android:layout_weight="1"
                    
                    android:orientation="vertical">
                    <ImageView 
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/listb"/>
                    <TextView 
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="网络"
                        android:textSize="15sp"
                        />
                 </LinearLayout>
              </LinearLayout>
              <LinearLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginBottom="10dp">
                <LinearLayout  
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="40dp"
                    android:layout_marginLeft="65dp"
                    android:background="#FFFFFF"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/listc" />
                    <TextView 
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="设置"
                        android:textSize="15sp"
                       />
                 </LinearLayout>
                 <LinearLayout  
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:background="#FFFFFF"
                    android:gravity="center">
                    <ImageView 
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/listd"/>
                    <TextView 
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="关闭"
                        android:textSize="15sp"
                        />
                 </LinearLayout>
              </LinearLayout>
              <LinearLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginBottom="10dp">
                <LinearLayout  
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="#FFFFFF"
                    android:layout_marginRight="40dp"
                    android:layout_marginLeft="65dp"
                    android:orientation="vertical">
                    <ImageView 
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/liste"/>
                    <TextView 
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="宝盒"
                        android:textSize="15sp"
                        />
                 </LinearLayout>
                 <LinearLayout  
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:background="#FFFFFF">
                    <ImageView 
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/listf"/>
                    <TextView 
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="查找"
                        android:textSize="15sp"
                        />
                 </LinearLayout>
              </LinearLayout>
              <LinearLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <LinearLayout  
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginRight="40dp"
                    android:layout_marginLeft="65dp"
                    android:gravity="center"
                    android:background="#FFFFFF"
                    android:orientation="vertical">
                    <ImageView 
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/listg"/>
                    <TextView 
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="下载"
                        android:textSize="15sp"
                        />
                 </LinearLayout>
                 <LinearLayout  
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:background="#FFFFFF"
                    android:orientation="vertical">
                    <ImageView 
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/listh"/>
                    <TextView 
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="记录"
                        android:textSize="15sp"
                        />
                 </LinearLayout>
              </LinearLayout>
              
            </LinearLayout>  
              
           
     
 
   


</RelativeLayout>
